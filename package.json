{
  "name": "ebay_search",
  "version": "1.0.0",
  "description": "Searches eBay for the sold prices of completed listings",
  "main": "main.js",
  "productName": "eBay Completed",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "package-mac": "electron-packager . --platform=darwin --arch=x64 --icon=assets/icons/logo.icns --prune=true --out=release-builds --darwinDarkModeSupport",
    "package-win": "electron-packager . --platform=win32 --arch=x64 --icon=assets/icons/logo.ico --prune=true --out=release-builds",
    "create": "webpack",
    "start": "webpack && electron .",
    "test": "webpack-dev-server"
  },
  "author": "Rafael Hernandez",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "electron": "^7.2.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^13.1.1",
    "history": "^4.9.0",
    "html-webpack-plugin": "^5.5.1",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-router-dom": "^4.3.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "resolutions": {
    "serialize-javascript": "^2.1.1"
  }
}
